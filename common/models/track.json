{
  "name": "Track",
  "plural": "tracks",
  "base": "PersistedModel",
  "idInjection": true,
  "mixins": {
    "AorMixin": {}
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "generated": true,
      "required": true
    },
    "name": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "duration": {
      "type": "number"
    },
    "path": {
      "type": "string",
      "required": true
    },
    "container": {
      "type": "string"
    },
    "processed": {
      "type": "boolean"
    },
    "error": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "playlist": {
      "type": "hasMany",
      "model": "Playlist",
      "foreignKey": "trackId"
    }
  },
  "acls": [{
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "accessType": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "ADMIN",
      "permission": "ALLOW",
      "accessType": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "accessType": "READ"
    }
  ],
  "methods": {
    "prototype.addToPlaylist": {
      "returns": {
        "arg": "playlist",
        "type": "object",
        "root": true
      }
    },
    "scanDir": {
      "returns": {
        "arg": "body",
        "type": "array",
        "root": true
      }
    }
  }
}
