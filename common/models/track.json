{
  "name": "Track",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "generated": true,
      "required": true
    },
    "name": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "duration": {
      "type": "number"
    },
    "path": {
      "type": "string",
      "required": true
    },
    "container": {
      "type": "string"
    },
    "processed": {
      "type": "boolean"
    },
    "error": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "playlist": {
      "type": "hasMany",
      "model": "Playlist",
      "foreignKey": "trackId"
    }
  },
  "acls": [],
  "methods": {
    "prototype.addToPlaylist": {
      "returns": {
        "arg": "playlist",
        "type": "object",
        "root": true
      }
    },
    "scanDir": {
      "returns": {
        "arg": "body",
        "type": "array",
        "root": true
      }
    }
  }
}
